    <filename>simple_jmenu/default.html</filename>
    <filename>simple_jmenu/js/simple_jmenu.js</filename>
    <filename>simple_jmenu/css/simple_jmenu.css</filename>
    <filename>simple_jmenu/images/bullet.gif</filename>
