   <filename>simple_jmenu/language/serbian.php</filename>
   <filename>simple_jmenu/language/srpski.php</filename>
   <filename>simple_jmenu/language/greek.php</filename>
   <filename>simple_jmenu/language/persian.php</filename>
   <filename>simple_jmenu/language/indonesian.php</filename>
   <filename>simple_jmenu/language/french.php</filename>
   <filename>simple_jmenu/language/russian.php</filename>
   <filename>simple_jmenu/language/german.php</filename>
   <filename>simple_jmenu/language/danish.php</filename>

