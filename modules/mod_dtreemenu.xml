	<option value="true">CX_true</option>
	<option value="false">CX_false</option>
	</param>
	<param name="closeSameLevel" type="radio" default="false" 
		label="CX_25"
		description="CX_26" >
	<option value="true">CX_true</option>
	<option value="false">CX_false</option>
