    <params>
	<param name="menutype" type="text" default="mainmenu"
		label="CX_01a"
		description="CX_01" />
	<param name="menutype" type="mos_menu" default="mainmenu" 
		label="CX_02"
		description="CX_03" />
	<param name="base" type="list" default="menu" 
		label="CX_04"
		description="CX_05" >
	<option value="first">CX_06</option>
	<option value="menu">CX_07</option>
	<option value="module">CX_08</option>
	<option value="site">CX_09</option>
	<option value="text">CX_10</option>
