	<params>
		<param name="moduleclass_sfx" type="text" default="" label="AX_SM_MCSL" description="AX_SM_MCSD" />
	  	<param name="menutype" type="mos_menu" default="mainmenu" label="AX_SM_MNM_MNL" description="AX_SM_MNM_MND" />
	  	<param name="menulayout" type="list" default="0" label="CX_EISFM_LAY" description="CX_EISFM_LAYD">
			<option value="0">CX_EISFM_LAYH</option>
			<option value="1">CX_EISFM_LAYV</option>
			<option value="4">CX_EISFM_LAYVR</option>
			<option value="2">CX_EISFM_LAYSL</option>
			<option value="3">CX_EISFM_LAYNAVB</option>
		</param>
	  	<param name="cssclass" type="text" default="" label="CX_EISFM_CLASSCSS" description="CX_EISFM_CLASSCSSD" />
	  	<param name="aligncenter" type="radio" default="0" label="CX_EISFM_ALIGN" description="CX_EISFM_ALIGND">
			<option value="0">AX_NO</option>
			<option value="1">AX_YES</option>
		</param>
		<param name="delay" type="text" default="1200" label="CX_EISFM_DELAY" description="CX_EISFM_DELAY" />
		<param name="speed" type="text" default="150" label="CX_EISFM_SPEED" description="CX_EISFM_SPEEDD" />
	  	<param name="arrow" type="radio" default="1" label="CX_EISFM_ARROW" description="CX_EISFM_ARROWD">
			<option value="0">AX_HIDE</option>
			<option value="1">AX_SHOW</option>
		</param>
	  	<param name="dropshadow" type="radio" default="1" label="CX_EISFM_SHADOW" description="CX_EISFM_SHADOWD">
			<option value="0">AX_HIDE</option>
			<option value="1">AX_SHOW</option>
		</param>
	</params>
